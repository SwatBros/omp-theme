{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": " {{ .Folder }}{{if .Root}} :: Admin{{end}}",
  "palette": {
    "main-bg": "#24283b",
    "terminal-red": "#f7768e",
    "pistachio-green": "#9ece6a",
    "terminal-green": "#73daca",
    "terminal-yellow": "#e0af68",
    "terminal-blue": "#7aa2f7",
    "celeste-blue": "#b4f9f8",
    "light-sky-blue": "#7dcfff",
    "terminal-white": "#c0caf5",
    "white-blue": "#a9b1d6",
    "blue-bell": "#9aa5ce",
    "pastal-grey": "#cfc9c2",
    "terminal-magenta": "#bb9af7",
    "blue-black": "#565f89",
    "terminal-black": "#414868",
    "t-background": "p:main-bg"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "background": "transparent",
          "foreground": "p:terminal-blue",
          "template": "{{ .Icon }} "
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "p:terminal-magenta",
          "properties": {
            "style": "folder"
          },
          "template": "<b>{{ .Path }}</b> <p:light-sky-blue>\uedd5</> "
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "p:terminal-green",
          "template": "(\udb85\udc0c {{ .FormattedMs }}) ",
          "properties": {
            "threshold": 500,
            "style": "austin",
            "always_enabled": true
          }
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "p:pistachio-green",
          "template": "\ue718 {{ .Full }} "
        },
{
          "type": "npm",
          "style": "plain",
          "foreground": "p:terminal-red",
          "template": "\ued0e {{ .Full }} "
        },
        {
          "type": "yarn",
          "style": "plain",
          "properties": {
            "extensions": ["yarn-lock.yaml"]
          },
          "foreground": "p:terminal-blue",
          "template": "\ue6a7 {{ .Full }} "
        },
        {
          "type": "pnpm",
          "style": "plain",
          "properties": {
            "extensions": ["pnpm-lock.yaml"]
          },
          "foreground": "p:terminal-yellow",
          "template": "\ue865 {{ .Full }} "
        },
        {
          "type": "bun",
          "style": "plain",
          "foreground": "p:pastal-grey",
          "template": "\ue76f {{ .Full }} "
        },
        {
          "type": "react",
          "style": "plain",
          "foreground": "p:terminal-blue",
          "template": "\ue7ba {{ .Full }} "
        },
        {
          "type": "php",
          "style": "plain",
          "foreground": "p:terminal-blue",
          "template": "\ue73d {{ .Full }} "
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "p:terminal-yellow",
          "template": "\uE235 {{ .Full }}"
        },
        {
          "type": "julia",
          "style": "plain",
          "foreground": "p:terminal-magenta",
          "template": "\uE624 {{ .Full }}"
        },
        {
          "type": "ruby",
          "style": "plain",
          "foreground": "p:terminal-red",
          "template": "\uE791 {{ .Full}}"
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "p:light-sky-blue",
          "template": "\uFCD1 {{ .Full}}"
        },
        {
          "type": "status",
          "style": "plain",
          "foreground": "p:terminal-red",
          "template": " \uf00d"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "overflow": "hide",
      "segments": [],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:terminal-yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0)}}p:light-sky-blue {{ end }}",
            "{{ if gt .Ahead 0 }}p:terminal-blue{{ end }}",
            "{{ if gt .Behind 0 }}p:celeste-blue{{ end }}"
          ],
          "foreground": "p:light-sky-blue",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "branch_icon": "\ue725 "
          },
          "style": "plain",
          "template": "â•°(<#ffffff>{{ .UpstreamIcon }} </>{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <#ffffff>\uf044</> {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <#ffffff>\uf046</> {{ .Staging.String }}{{ end }})",
          "type": "git"
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "p:light-sky-blue",
          "properties": {
            "fetch_user": true
          },
          "template": " \udb81\udf1b {{ .User.Email }} "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:pistachio-green",
          "style": "plain",
          "template": "\u279c",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "p:terminal-blue",
    "template": "\u279c "
  },
  "transient_prompt": {
    "foreground": "p:terminal-magenta",
    "template": "\u279c "
  },
  "final_space": true,
  "version": 3,
  "terminal_background": "p:t-background"
}
